{
  "name": "uristate",
  "version": "1.0.0",
  "description": "URI State",
  "engines": {
    "iojs": "1.6.2"
  },
  "scripts": {
    "start": "node --es_staging --harmony_arrow_functions server.js",
    "build": "rm -rf public && cp -pr src/static/ public && webpack",
    "lint": "npm run lint:node && npm run lint:browser",
    "lint:node": "eslint --config=./.eslintrc *.js scripts/*.js",
    "lint:browser": "eslint --config=./src/.eslintrc src/**/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nkbt/uristate.git"
  },
  "bugs": {
    "url": "https://github.com/nkbt/uristate/issues"
  },
  "dependencies": {
    "babel-core": "^4.7.16",
    "babel-loader": "^4.3.0",
    "css-loader": "^0.9.1",
    "eslint": "^0.19.0",
    "eslint-loader": "^0.10.0",
    "eslint-plugin-react": "^2.1.0",
    "file-loader": "^0.8.1",
    "flux": "^2.0.1",
    "immutable": "^3.7.2",
    "lodash": "^3.6.0",
    "object-hash": "^0.5.0",
    "react": "^0.13.1",
    "react-hot-loader": "^1.2.4",
    "react-jss": "^0.1.3",
    "react-router": "^0.13.2",
    "sorted-object": "^1.0.0",
    "style-loader": "^0.9.0",
    "url-loader": "^0.5.5",
    "uuid": "^2.0.1",
    "webpack": "^1.8.4",
    "webpack-dev-server": "^1.8.0"
  }
}
